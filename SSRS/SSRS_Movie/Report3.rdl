  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Distinct DateDim.Year, MovieDim.MName,MovieDim.Mrating, sum (Movie_fact.TotalSales) as Total_Sales
FROM     DateDim INNER JOIN
                  Movie_fact ON DateDim.DateKey = Movie_fact.DateKey INNER JOIN
                  MovieDim ON Movie_fact.MovieKey = MovieDim.MovieKey INNER JOIN
                  SalesDim ON Movie_fact.SalesKey = SalesDim.SalesKey
group by DateDim.Year, MovieDim.MName,MovieDim.Mrating 
order by Mrating desc ;</CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MName">
          <DataField>MName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Mrating">
          <DataField>Mrating</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Total_Sales">
          <DataField>Total_Sales</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
