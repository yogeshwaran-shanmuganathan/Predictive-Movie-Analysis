  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Distinct MovieDim.MName, LocationDim.Region, sum(Movie_fact.UnitsSold) as UnitsSold
FROM     LocationDim INNER JOIN
                  Movie_fact ON LocationDim.LocationKey = Movie_fact.LocationKey INNER JOIN
                  MovieDim ON Movie_fact.MovieKey = MovieDim.MovieKey INNER JOIN
                  SalesDim ON Movie_fact.SalesKey = SalesDim.SalesKey
Group by LocationDim.Region, MovieDim.MName;</CommandText>
      </Query>
      <Fields>
        <Field Name="UnitsSold">
          <DataField>UnitsSold</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Region">
