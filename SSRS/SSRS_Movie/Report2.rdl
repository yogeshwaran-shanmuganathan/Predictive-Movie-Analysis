    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Distinct CastDim.AFirstName, CastDim.ALastName, MovieDim.Mgenre
FROM     CastDim INNER JOIN
                  Movie_fact ON CastDim.CastKey = Movie_fact.CastKey INNER JOIN
                  MovieDim ON Movie_fact.MovieKey = MovieDim.MovieKey
Group by CastDim.AFirstName, CastDim.ALastName, MovieDim.Mgenre;</CommandText>
